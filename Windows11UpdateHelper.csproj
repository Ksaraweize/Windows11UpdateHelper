<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<VersionPrefix>0.0.1</VersionPrefix>
		<Authors>https://github.com/xiao-cao-x</Authors>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<PlatformTarget>x64</PlatformTarget>
		<SignAssembly>False</SignAssembly>
		<Copyright>https://github.com/xiao-cao-x/Windows11UpdateHelper</Copyright>
		<RepositoryUrl>https://github.com/xiao-cao-x/windows11-bypass-tpm-uefi-update</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<ApplicationIcon>icon.ico</ApplicationIcon>
		<DebugType>none</DebugType>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="icon.ico" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="HandyControl" Version="3.3.0" />
		<PackageReference Include="Microsoft.Toolkit.Mvvm" Version="7.1.2" />
		<PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
	</ItemGroup>

	<ItemGroup>
		<None Update="Views\HelperPage.xaml">
			<Generator>MSBuild:Compile</Generator>
		</None>
	</ItemGroup>

</Project>
